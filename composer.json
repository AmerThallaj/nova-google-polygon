{
    "name": "yieldstudio/nova-google-polygon",
    "description": "A Laravel Nova Google polygon field.",
    "keywords": [
        "yieldstudio",
        "laravel",
        "nova",
        "google",
        "polygon",
        "area"
    ],
    "homepage": "https://github.com/YieldStudio/nova-google-polygon",
    "license": "MIT",
    "authors": [
        {
            "name": "James Hemery",
            "email": "james@yieldstudio.fr",
            "homepage": "https://yieldstudio.fr",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "YieldStudio\\NovaGooglePolygon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YieldStudio\\NovaGooglePolygon\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8"
    }
}
